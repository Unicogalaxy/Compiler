int main() {
  int a = 10;
  int b = 20;
  int c = 1; 
  int result = 0;

  // Test 1: && and >
  if (a > 5 && b > 15) {
    result = result + 10; // result = 10
  }

  // Test 2: || and <
  if (a < 5 || b > 15) {
    result = result + 20; // result = 30
  }

  // Test 3: ! (Not) and ==
  if (!(a == b)) {
    result = result + 5;  // result = 35
  }

  // Test 4: Nested if and short-circuit &&
  // a > 20 is false, so the second part (c == 1) should not be evaluated.
  if (a > 20 && c == 1) {
    result = result + 100; // This should not execute
  } else {
    result = result + 7;   // result = 42
  }

  return result;
}